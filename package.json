{
  "name": "katex-macro-compiler",
  "version": "1.0.0",
  "description": "Transpile LaTeX macro definitions to KaTeX-compatible macro object",
  "exports": {
    ".": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && rolldown -c",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "test": "vitest run",
    "prepublishOnly": "pnpm format && pnpm lint && pnpm test && pnpm build"
  },
  "keywords": [
    "LaTeX",
    "KaTeX",
    "macro"
  ],
  "author": "Ryota Ushio",
  "license": "MIT",
  "packageManager": "pnpm@10.21.0",
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.0",
    "arkregex": "^0.0.2",
    "eslint": "^9.39.1",
    "prettier": "^3.6.2",
    "rolldown": "1.0.0-beta.47",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.3",
    "vitest": "^4.0.8"
  }
}
